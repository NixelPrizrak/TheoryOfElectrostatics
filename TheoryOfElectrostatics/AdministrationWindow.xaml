<local:PatternWindow x:Class="TheoryOfElectrostatics.AdministrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheoryOfElectrostatics"
        mc:Ignorable="d"
        Title="AdministrationWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <DockPanel Background="AliceBlue">
        <Grid DockPanel.Dock="Top" Height="60">
            <Button Content="Лекции" Margin="186,0,0,0" Height="30" HorizontalAlignment="Left" Width="160" VerticalAlignment="Center" FontFamily="Sitka Text" FontSize="16" Click="LectionsButton_Click"/>
            <Button Content="Тесты" Height="30" HorizontalAlignment="Left" Width="160" VerticalAlignment="Center" FontFamily="Sitka Text" FontSize="16" Margin="485,0,0,0" Click="TestsButton_Click"/>
        </Grid>
        <Frame x:Name="AdministrationFrame"/>

    </DockPanel>
</local:PatternWindow>
